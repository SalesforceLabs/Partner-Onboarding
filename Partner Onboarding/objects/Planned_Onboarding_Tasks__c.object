  &quot;Overdue&quot;,
  IF(
    Task_Due_Date__c == today() &amp;&amp; NOT(ISPICKVAL(Status__c, &apos;Completed&apos;)), 
    &quot;Due&quot;,
    TEXT(Status__c)
